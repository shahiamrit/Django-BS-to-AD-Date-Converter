    "import cv2\n",
    "path = \"./IMG_4821.JPG\"\n",
    "img = cv2.imread(path)\n",
    "width, height = 600 , 600\n",
    "imgResize = cv2.resize(img, (width, height))\n",
    "print(imgResize.shape)\n",
    "\n",
    "imgCropped = img[0:600, 0:600]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "cv2.imshow(\"Road\", imgResize)\n",
    "cv2.imshow(\"Road Cropped\", imgCropped)\n",
    "cv2.waitKey(0)\n"
   ]
